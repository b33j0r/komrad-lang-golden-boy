agent Response {
	[write _html] {
		Io println "HTTP/1.1"
		Io println html
	}
}

agent Server {
	[http _response GET "/"] {
		html = ```html
	  <!DOCTYPE html>
	  <html>
	  <head>
	      <title>Komrad</title>
	  </head>
	  <body>
	    Komrad
	  </body>
	  </html>
	  ```
	  response write html
  }
}

[main] {
	server = spawn Server {}
	response = spawn Response {}
	server http response GET "/"
}